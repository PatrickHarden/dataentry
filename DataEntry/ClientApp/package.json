  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start": "run-p lint start:dev",
    "start:dev": "node scripts/start.js",
    "lint": "tslint src/**/*.{ts,tsx}",
    "build": "node scripts/build.js",
    "postbuild": "node postbuild.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:ci": "node scripts/test.js --env=jsdom --coverage --ci  --reporters=default --reporters=jest-junit"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "jest-junit": {
    "suiteName": "{filename}",
    "output": "./test-reports/junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " ï¿½ ",
    "usePathForSuiteName": "true"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!node_modules/",
      "!src/locales/**/*.*",
      "!src/registerServiceWorker.js",
      "!src/state/reducers/index.js",
      "!src/state/selectors/index.js",
      "!src/stories/**/*.*",
      "!src/utils/**/*.*",
      "!src/routes.jsx",
      "!src/App.jsx",
      "!src/index.js",
      "!src/modules/**/selectors/index.js",
      "!src/state/createStore.js",
      "!src/state/initialState.js",
      "!src/**/*.stories.{ts,tsx}",
      "!src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^dnd-core$": "dnd-core/dist/cjs",
      "^react-dnd$": "react-dnd/dist/cjs",
      "^react-dnd-html5-backend$": "react-dnd-html5-backend/dist/cjs",
      "^react-dnd-touch-backend$": "react-dnd-touch-backend/dist/cjs",
      "^react-dnd-test-backend$": "react-dnd-test-backend/dist/cjs",
      "^react-dnd-test-utils$": "react-dnd-test-utils/dist/cjs"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
